|    CREATE TABLE [dbo].[users](
    	[username] [varchar](50) NOT NULL,
    	[password] [varchar](50) NOT NULL
    )
    
    CREATE TABLE [dbo].[user_roles](
    	[username] [varchar](50) NOT NULL,
    	[role_name] [varchar](50) NOT NULL
    )
    
    CREATE TABLE [dbo].[roles_permissions](
    	[role_name] [varchar](50) NOT NULL,
    	[permission] [varchar](50) NOT NULL
    )
